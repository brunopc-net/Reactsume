{
  "name": "reactsume",
  "version": "1.0.0",
  "description": "React based resume with custom themes and multilanguage support ",
  "author": "Bruno Pettersen-Coulombe",
  "repository": {
    "type": "git",
    "url": "https://github.com/brunopc-net/Reactsume"
  },
  "homepage": "https://brunopc-net.github.io/Reactsume",
  "license": "MIT",
  "main": "src/index.js",
  "dependencies": {
    "reactsume-theme-bufferbloat": "^1.1.4",
    "gh-pages": "^6.1.1",
    "react": "^18.3.1",
    "react-document-meta": "^3.0.0-beta.2",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
